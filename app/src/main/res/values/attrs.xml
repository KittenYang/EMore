<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ForegroundView">
        <attr name="android:foreground" />
        <attr name="android:foregroundInsidePadding" />
        <attr name="android:foregroundGravity" />
    </declare-styleable>

    <declare-styleable name="ScrimInsetsView">
        <attr name="appInsetForeground" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="SubsamplingScaleImageView">
        <attr name="src" format="reference"/>
        <attr name="assetName" format="string"/>
        <attr name="panEnabled" format="boolean"/>
        <attr name="zoomEnabled" format="boolean"/>
        <attr name="quickScaleEnabled" format="boolean"/>
        <attr name="tileBackgroundColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="RatioView">
        <attr name="ratio" format="float"/>
    </declare-styleable>
</resources>